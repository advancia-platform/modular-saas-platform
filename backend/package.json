{
  "name": "advancia-backend",
  "version": "1.0.0",
  "private": true,
  "type": "commonjs",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "start": "node dist/index.js",
    "dev": "nodemon --watch src --ext ts,js --ignore '*.test.ts' --ignore '*.spec.ts' --delay 1000ms --exec ts-node src/index.ts",
    "dev:fast": "nodemon --watch src --ext ts,js --ignore '*.test.ts' --ignore '*.spec.ts' --delay 500ms --exec ts-node --transpile-only src/index.ts",
    "dev:watch": "tsc --watch --incremental --tsBuildInfoFile .tsbuildinfo",
    "dev:watch:start": "concurrently \"npm run dev:watch\" \"nodemon --delay 2000ms dist/index.js\"",
    "dev:trust": "ts-node src/index.trust.ts",
    "dev:minimal": "ts-node src/index.minimal.ts",
    "worker": "ts-node src/workers/notificationWorker.ts",
    "build": "node build.js",
    "build:watch": "tsc --watch",
    "build:prod": "tsc && npm run build:optimize",
    "build:optimize": "node --max-old-space-size=4096 build.js",
    "lint": "eslint src --ext .ts,.js",
    "lint:check": "eslint src --ext .ts,.js",
    "lint:strict": "eslint src --ext .ts,.js --max-warnings 0",
    "lint:fix": "eslint src --ext .ts,.js --fix",
    "type-check": "tsc --project tsconfig.json --noEmit",
    "check": "npm run type-check",
    "check:test": "tsc --project tsconfig.test.json --noEmit",
    "migrate:deploy": "npx prisma migrate deploy",
    "pm2:start": "pm2 start ecosystem.config.js",
    "pm2:stop": "pm2 stop ecosystem.config.js",
    "pm2:restart": "pm2 restart ecosystem.config.js",
    "pm2:delete": "pm2 delete ecosystem.config.js",
    "pm2:logs": "pm2 logs advancia-backend",
    "pm2:monit": "pm2 monit",
    "migrate": "node src/index.js migrate",
    "seed": "node src/seed.js",
    "test": "cross-env NODE_ENV=test jest --config jest.config.js",
    "test:watch": "npm test -- --watch",
    "test:coverage": "npm test -- --coverage",
    "test:verbose": "npm test -- --verbose",
    "test:smoke": "cross-env NODE_ENV=test node --experimental-vm-modules node_modules/.bin/jest --config jest.config.js --testPathPattern=smoke",
    "migrate:test": "cross-env NODE_ENV=test DATABASE_URL=\"$TEST_DATABASE_URL\" npx prisma migrate deploy",
    "seed:test": "cross-env NODE_ENV=test DATABASE_URL=\"$TEST_DATABASE_URL\" node scripts/seed-test-data.js",
    "db:setup:test": "node scripts/setup-test-db-local.js",
    "db:setup:test:docker": "node scripts/setup-test-db-docker.js",
    "db:setup:test:simple": "node scripts/setup-test-db-simple.js",
    "db:setup:test:full": "node scripts/setup-test-db.js",
    "db:seed": "ts-node prisma/seed.ts",
    "db:reset:test": "cross-env NODE_ENV=test DATABASE_URL=\"$TEST_DATABASE_URL\" npx prisma migrate reset --force --skip-seed",
    "test:db": "node scripts/test-db-runner.js",
    "test:db:setup": "node scripts/test-db-runner.js --setup",
    "test:db:reset": "node scripts/test-db-runner.js --reset",
    "test:db:seed": "node scripts/test-db-runner.js --seed",
    "test:db:health": "node scripts/test-db-runner.js --health",
    "test:db:benchmark": "node scripts/test-db-runner.js --benchmark",
    "test:db:cleanup": "node scripts/test-db-runner.js --cleanup",
    "prisma:generate": "npx prisma generate",
    "prisma:studio": "npx prisma studio",
    "prisma:studio:test": "cross-env DATABASE_URL=\"$TEST_DATABASE_URL\" npx prisma studio",
    "gen:password": "ts-node scripts/generate-password.ts",
    "gen:secret": "ts-node scripts/generate-password.ts --hex 64",
    "generate:secrets": "ts-node scripts/generate-secrets.ts",
    "verify:env": "ts-node scripts/verify-env.ts",
    "verify:db": "ts-node scripts/verify-migrations.ts",
    "verify:security": "ts-node scripts/security-audit.ts",
    "verify:api": "ts-node scripts/verify-api.ts",
    "verify:production": "ts-node scripts/verify-production.ts",
    "eval:install": "python -m pip install -r security-tests/requirements.txt",
    "eval:setup": "node -e \"require('fs').copyFileSync('security-tests/.env.example', 'security-tests/.env')\"",
    "eval:diagnose": "cd security-tests && pwsh -ExecutionPolicy Bypass -File diagnose.ps1",
    "eval:cleanup": "cd security-tests && pwsh -ExecutionPolicy Bypass -File cleanup-duplicates.ps1",
    "eval:run": "python security-tests/run_all_evaluators.py --verbose",
    "eval:test": "npm run eval:run",
    "eval:password": "python security-tests/run_all_evaluators.py --test password --verbose",
    "eval:auth": "python security-tests/run_all_evaluators.py --test auth_flow --verbose",
    "eval:rate-limit": "python security-tests/run_all_evaluators.py --test rate_limiting --verbose",
    "eval:report": "python security-tests/run_all_evaluators.py --report html --output results/security_report",
    "eval:ci": "python security-tests/run_all_evaluators.py --report json --output results/ci-evaluation",
    "db:test": "ts-node src/utils/testDatabaseConnection.ts",
    "db:migrate:prod": "bash scripts/migrate-production.sh",
    "db:studio:prod": "DATABASE_URL=$DATABASE_URL npx prisma studio",
    "db:health": "curl http://localhost:4000/api/system/db-health",
    "admin": "ts-node scripts/admin.ts",
    "admin:create": "ts-node scripts/admin.ts create-admin",
    "admin:list": "ts-node scripts/admin.ts list-users",
    "admin:stats": "ts-node scripts/admin.ts stats",
    "test:telegram": "npx tsx test-telegram-direct.ts",
    "test:sms": "npx tsx test-sms-direct.ts",
    "test:whatsapp": "npx tsx test-whatsapp-direct.ts",
    "test:communications": "npx tsx test-all-communications.ts"
  },
  "dependencies": {
    "@ai-sdk/openai": "^2.0.71",
    "@aws-sdk/client-s3": "^3.490.0",
    "@aws-sdk/s3-request-presigner": "^3.490.0",
    "@datadog/datadog-api-client": "^1.47.0",
    "@faker-js/faker": "^9.9.0",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/auto-instrumentations-node": "^0.54.0",
    "@opentelemetry/exporter-trace-otlp-http": "^0.54.0",
    "@opentelemetry/instrumentation": "^0.54.0",
    "@opentelemetry/instrumentation-express": "^0.54.0",
    "@opentelemetry/instrumentation-http": "^0.54.0",
    "@opentelemetry/sdk-node": "^0.54.0",
    "@prisma/adapter-pg": "^7.0.0",
    "@prisma/instrumentation": "^5.21.1",
    "@sentry/node": "^10.25.0",
    "@sentry/profiling-node": "^10.25.0",
    "@sentry/tracing": "^7.120.4",
    "@types/bcrypt": "^6.0.0",
    "@types/bull": "^3.15.9",
    "@types/json2csv": "^5.0.7",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/nodemailer": "^7.0.4",
    "ai": "^5.0.98",
    "amqplib": "^0.10.9",
    "argon2": "^0.44.0",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^2.4.3",
    "bip32": "^5.0.0",
    "bip39": "^3.1.0",
    "bitcoinjs-lib": "^7.0.0",
    "bull": "^4.16.5",
    "chart.js": "^4.5.1",
    "cors": "2.8.5",
    "dotenv": "16.4.5",
    "ethers": "^6.15.0",
    "express": "^5.1.0",
    "express-rate-limit": "^8.2.1",
    "express-validator": "^7.3.1",
    "helmet": "7.0.0",
    "https-proxy-agent": "^7.0.5",
    "ioredis": "^5.8.2",
    "jose": "^6.1.2",
    "json2csv": "^5.0.7",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.3",
    "node-telegram-bot-api": "^0.66.0",
    "nodemailer": "^7.0.10",
    "pdfkit": "^0.15.0",
    "pg": "8.13.1",
    "prom-client": "^15.1.3",
    "prometheus-gc-stats": "^1.1.0",
    "qrcode": "^1.5.4",
    "react-chartjs-2": "^5.3.1",
    "redis": "^5.9.0",
    "resend": "^6.5.2",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "socks-proxy-agent": "^8.0.4",
    "speakeasy": "^2.0.0",
    "stripe": "^20.0.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "tiny-secp256k1": "^2.2.4",
    "validator": "^13.15.23",
    "web-push": "^3.6.7",
    "winston": "^3.18.3",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@prisma/client": "^7.0.0",
    "@types/amqplib": "^0.10.8",
    "@types/bcryptjs": "^2.4.6",
    "@types/express": "^4.17.21",
    "@types/ioredis": "^4.28.10",
    "@types/jest": "^29.5.11",
    "@types/multer": "^2.0.0",
    "@types/node": "^20.19.25",
    "@types/node-cron": "^3.0.11",
    "@types/pdfkit": "^0.13.5",
    "@types/supertest": "^2.0.16",
    "@types/validator": "^13.15.10",
    "@types/web-push": "^3.6.4",
    "@typescript-eslint/eslint-plugin": "^8.47.0",
    "@typescript-eslint/parser": "^8.47.0",
    "axios": "^1.13.2",
    "chai": "4.3.10",
    "cross-env": "^10.1.0",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "globals": "^14.0.0",
    "jest": "^29.7.0",
    "mocha": "^10.8.2",
    "nodemon": "3.0.2",
    "pm2": "^6.0.13",
    "prettier": "^3.6.2",
    "prisma": "^7.0.0",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}
