{
  "name": "@advancia/ai-devops-agent",
  "version": "1.0.0",
  "description": "Revolutionary AI DevOps Agent that transforms fintech AI functions into intelligent error detection and automated fix system",
  "main": "ai-core/AIDevOpsAgent.ts",
  "scripts": {
    "start": "node -r ts-node/register ai-core/launcher.js",
    "dev": "nodemon --exec ts-node ai-core/launcher.js",
    "demo": "node -r ts-node/register demo/AIDevOpsAgentDemo.ts",
    "demo:full": "ts-node demo/AIDevOpsAgentDemo.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "build": "tsc",
    "lint": "eslint --ext .ts,.js .",
    "lint:fix": "eslint --ext .ts,.js . --fix",
    "analyze": "python ai-core/reasoning/analyze_error.py",
    "generate-fix": "python ai-core/reasoning/generate_fix_plan.py",
    "setup": "npm install && pip install -r requirements.txt",
    "docker:build": "docker build -t advancia/ai-devops-agent .",
    "docker:run": "docker run -p 3000:3000 -p 5000:5000 advancia/ai-devops-agent"
  },
  "keywords": [
    "ai",
    "devops",
    "automation",
    "fintech",
    "error-detection",
    "intelligent-fixes",
    "machine-learning",
    "cicd",
    "monitoring",
    "fraud-detection",
    "risk-assessment",
    "algorithmic-trading",
    "automated-testing"
  ],
  "author": "Advancia Platform Team <team@advancia.tech>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/advancia-platform/ai-devops-agent.git"
  },
  "bugs": {
    "url": "https://github.com/advancia-platform/ai-devops-agent/issues"
  },
  "homepage": "https://github.com/advancia-platform/ai-devops-agent#readme",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "dependencies": {
    "@types/node": "^20.10.0",
    "axios": "^1.6.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "node-cron": "^3.0.3",
    "openai": "^4.20.0",
    "socket.io": "^4.7.4",
    "socket.io-client": "^4.7.4",
    "typescript": "^5.3.0",
    "uuid": "^9.0.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.8",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/lodash": "^4.14.202",
    "@types/node-cron": "^3.0.11",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1"
  },
  "peerDependencies": {},
  "files": [
    "ai-core/",
    "demo/",
    "docs/",
    "examples/",
    "k8s/",
    "requirements.txt",
    "Dockerfile",
    "docker-compose.yml",
    "README.md",
    "AI_DEVOPS_AGENT_README.md",
    "AI_DEVOPS_AGENT_ARCHITECTURE.md"
  ],
  "config": {
    "error_sources": [
      "github",
      "sentry",
      "prometheus",
      "security_scans"
    ],
    "ai_models": [
      "gpt-4",
      "gpt-3.5-turbo"
    ],
    "risk_thresholds": {
      "auto_fix": 0.8,
      "human_review": 0.6,
      "critical_alert": 0.9
    }
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  }
}
