# ðŸ“„ Frontend .env.production.example
# Copy this to .env.production and fill in real values
# NEVER commit .env.production - it's in .gitignore

# ==========================================
# API Configuration
# ==========================================
NEXT_PUBLIC_API_URL=https://<YOUR_BACKEND_DOMAIN>

# ==========================================
# Stripe Public Key
# ==========================================
NEXT_PUBLIC_STRIPE_PUBLISHABLE_KEY=<YOUR_STRIPE_PUBLISHABLE_KEY>

# ==========================================
# Monitoring & Analytics
# ==========================================
NEXT_PUBLIC_SENTRY_DSN=<YOUR_SENTRY_DSN>
NEXT_PUBLIC_GA_TRACKING_ID=<YOUR_GA_ID_HERE>

# ==========================================
# Feature Flags
# ==========================================
NEXT_PUBLIC_FF_GRAPHQL=false
NEXT_PUBLIC_FF_EXPERIMENTAL_FEATURES=false

# ==========================================
# WebSocket Configuration
# ==========================================
NEXT_PUBLIC_WS_URL=wss://<YOUR_BACKEND_DOMAIN>

# ==========================================
# Environment
# ==========================================
NODE_ENV=production
